{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // https://code.visualstudio.com/docs/editor/tasks-v1
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compile and upload",
            "type": "shell",
            "command": "make -C ${workspaceFolder}",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "compile",
            "type": "shell",
            "command": "make -C ${workspaceFolder} compile",
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "RSTDISBL",
            "type": "shell",
            "command": "make -C ${workspaceFolder} rstdisbl",
            "problemMatcher": []
        },
        {
            "label": "clean",
            "type": "shell",
            "command": "make -C ${workspaceFolder} clean",
            "problemMatcher": []
        }
    ]
}